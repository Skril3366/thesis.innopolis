@article{meleExperimentingMetaverseFoster2024,
  title = {Experimenting on the Metaverse to Foster Innovation},
  author = {Mele, Cristina and Spena, Tiziana Russo and Russo, Stefano Paolo},
  year = {2024},
  month = jul,
  journal = {Spanish Journal of Marketing - ESIC},
