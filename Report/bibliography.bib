@article{dholiBlockchainApplicationUsing2023,
  title = {Blockchain {{Application Using NFT Marketplace}}},
  author = {Dholi, Aryan and Mandawade, Pratiksha and Pagare, Nisha and Bhangale, Kuldeep and Nalge, Prof},
  year = {2023},
