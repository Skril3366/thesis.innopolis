@article{gaoEvolutionaryGameAnalysis2023,
  title = {Evolutionary {{Game Analysis}} of {{Copyright Protection}} for {{NFT Digital Works Considering Collusive Behavior}}},
  author = {Gao, Yudong and Xie, Xuemei and Ni, Yuan},
