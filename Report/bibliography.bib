  author = {{Matthieu Nadini} and Nadini, Matthieu and {Laura Alessandretti} and Alessandretti, Laura and {Flavio Di Giacinto} and {Flavio Di Giacinto} and Di Giacinto, Flavio and {Mauro Martino} and Martino, Mauro and Aiello, Luca Maria and {Luca Maria Aiello} and Aiello, Luca Maria and {Andrea Baronchelli} and Baronchelli, Andrea},
